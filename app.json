{
  "expo": {
    "name": "ticketworldapp",
    "slug": "ticketworldapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "fonts": {
      "RedditSansCondensed-Regular": "./assets/fonts/RedditSansCondensed-Regular-BF65163a1ab2dba.ttf",
      "RedditSansCondensed-Bold": "./assets/fonts/RedditSansCondensed-Bold-BF65163a1aab219.ttf",
      "RedditSansCondensed-SemiBold": "./assets/fonts/RedditSansCondensed-SemiBold-BF65163a1acd047.ttf",
      "RedditSansCondensed-Light": "./assets/fonts/RedditSansCondensed-Light-BF65163a1abfd6e.ttf",
      "RedditSansCondensed-ExtraBold": "./assets/fonts/RedditSansCondensed-ExtraBold-BF65163a1aae032.ttf"
    }
  }
}
